{

  "name": "RbxFlip - The new way to win coinflips!",
  "description": "A chrome extension for https://rbxflip.com/ and beta https://beta.rbxflip.com/",
  "version": "1.1.12",
  "manifest_version": 2,

  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "Predict The Flip!",
    "default_popup": "newCoin.html"
  },

    "permissions": [
        "cookies",
        "https://*/*"

    ],

    "background": {
      "scripts": ["predict.js"]
  }
}
